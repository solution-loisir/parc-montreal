<h1>Parcs {{ title }}</h1>
<ul>
{% for p in parc.result.records %}
{% if p['GESTION'] === 'Ahuntsic-Cartierville' %}
<li style="color:DarkBlue;">
{{ p['Nom'] }}
</li>
{% for i in parcInstallations.features %}
{% if (i.properties.INDEX_PARC === p.NUM_INDEX) and i.properties.TYPE === 'Sportif' %}
